Int main () {
	Int a = -3;
	Int b = 10;
	If (a <=  b) {
		a = a + b;
	else
		return b – a;
	}
}


•	Paso 1.
a = %L0

b = %L1



•	Paso 2.
0x0000    MOV    -3    %L0    ---- > 1

0x0004    MOV    10    %L1    ---- > 2

0x0008    SUBcc  %L0    %L1    %G0    ---- > 3

0x000c    BG    a    FALSE    ---- > 4
0x0010    ADD  %L0    %L1    %L0    ---- > 5
0x0014    BA        EXIT    ---- > 6
FALSE    
0x0018    SUB  %L1    %L0    %O0    ---- > 7 
EXIT    
	NOP    



•	Paso 3.
