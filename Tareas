0x000c    BG    a    FALSE    ---- > 4
0x0010    ADD  %L0    %L1    %L0    ---- > 5
0x0014    BA        EXIT    ---- > 6
FALSE    
0x0018    SUB  %L1    %L0    %O0    ---- > 7 
EXIT    
	NOP    



